<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="300.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fxControllers.UpdateStatusWindow">
   <children>
      <Text layoutX="45.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Update Status">
         <font>
            <Font name="System Bold" size="24.0" />
