            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="33.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current status:" />
      <Text fx:id="currentStatusText" layoutX="134.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="currentStatusText" />
      <Text layoutX="75.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select new status:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
      <ComboBox fx:id="statusChoserComboBox" layoutX="50.0" layoutY="181.0" prefWidth="150.0" />
